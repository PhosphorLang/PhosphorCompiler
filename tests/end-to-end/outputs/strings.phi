constant c#0 "abc"
constant c#1 "def"

external stringsAreEqual (Pointer, Pointer): Int8

function testStringCompare (Pointer, Pointer): Int8
{
    introduce v#0
    take v#0 p#0
    introduce v#1
    take v#1 p#1
    introduce v#3
    move v#3 v#0
    dismiss v#0
    give p#0 v#3
    dismiss v#3
    introduce v#4
    move v#4 v#1
    dismiss v#1
    give p#1 v#4
    dismiss v#4
    call stringsAreEqual
    introduce v#2
    take v#2 r#0
    give r#0 v#2
    dismiss v#2
    return
}

function main (): Void
{
    introduce v#5
    move v#5 c#0
    give p#0 v#5
    dismiss v#5
    introduce v#6
    move v#6 c#1
    give p#1 v#6
    dismiss v#6
    call testStringCompare
    introduce v#7
    move v#7 c#0
    give p#0 v#7
    dismiss v#7
    introduce v#8
    move v#8 c#0
    give p#1 v#8
    dismiss v#8
    call testStringCompare
    return
}
